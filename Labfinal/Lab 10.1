#include <stdio.h>
#include <ctype.h>
#include <string.h>

int main(void) {
    char input[1024];
    char normalized[1024];

    printf("Enter word:\n");
    if (!fgets(input, sizeof(input), stdin)) return 0;

    size_t len = strlen(input);
    if (len && input[len - 1] == '\n') input[len - 1] = '\0';

    int j = 0;
    for (size_t i = 0; input[i] != '\0'; ++i) {
        if (isalnum((unsigned char)input[i])) {
            normalized[j++] = (char)tolower((unsigned char)input[i]);
        }
    }
    normalized[j] = '\0';

    int left = 0;
    int right = j - 1;
    int is_pal = 1;

    while (left < right) {
        if (normalized[left] != normalized[right]) {
            is_pal = 0;
            break;
        }
        ++left;
        --right;
    }

    if (is_pal) {
        printf("Pass.\n");
    } else {
        printf("Not Pass.\n");
    }

    return 0;
}
