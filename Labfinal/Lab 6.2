#include <stdio.h>

int main(void) {
    int N;
    int first = 1;

    printf("Enter value:\n");
    if (scanf("%d", &N) != 1) return 0;

    printf("Series: ");

    if (N % 2 != 0) {
        for (int i = 1; i <= N; i += 2) {
            if (!first) printf(" ");
            printf("%d", i);
            first = 0;
        }
    } else {
        for (int i = N; i >= 0; i -= 2) {
