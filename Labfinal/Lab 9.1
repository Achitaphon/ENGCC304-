#include <stdio.h>
#include <math.h>

int isPrime(int x) {
    if (x < 2) return 0;
    if (x % 2 == 0) return x == 2;
    for (int d = 3; d <= (int)sqrt((double)x); d += 2) {
        if (x % d == 0) return 0;
    }
    return 1;
}

int main(void) {
    int N;

    printf("Enter N :\n");
    if (scanf("%d", &N) != 1 || N <= 0) return 0;

    int arr[N];
    for (int i = 0; i < N; ++i) {
        printf("Enter value[%d] :\n", i);
        if (scanf("%d", &arr[i]) != 1) return 0;
    }

    printf("Index: ");
    for (int i = 0; i < N; ++i) {
        printf("%d", i);
        if (i < N - 1) printf(" ");
    }
    printf("\n");

    printf("Array: ");
    for (int i = 0; i < N; ++i) {
        if (isPrime(arr[i])) {
            printf("%d", arr[i]);
        } else {
            printf("#");
        }
        if (i < N - 1) printf(" ");
    }
    printf("\n");

    return 0;
}
