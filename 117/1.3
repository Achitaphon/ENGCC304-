#include <stdio.h>
#include <stdlib.h>

int *GetSet(int *n); // ประกาศโปรโตไทป์ของฟังก์ชัน

int main() {
    int *data, num, i;

    data = GetSet(&num);

    printf("\nMembers in set:\n");
    for (i = 0; i < num; i++) {
        printf("%d ", data[i]);
    }

    free(data);
    return 0;
}

int *GetSet(int *n) {
    int i;
    int *set;

    printf("Enter number of members: ");
    scanf("%d", n);

    set = (int *)malloc((*n) * sizeof(int));

    if (set == NULL) {
        printf("Memory allocation failed!\n");
        exit(1);
    }

    for (i = 0; i < *n; i++) {
        printf("Enter member %d: ", i + 1);
        scanf("%d", &set[i]);
    }

    return set;
}
