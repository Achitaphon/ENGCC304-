#include <stdio.h>
#include <stdlib.h>

int *GetSet(int *n);

int main(void)
{
    int *data;
    int num;

    data = GetSet(&num);

    free(data);
    return 0;
}

int *GetSet(int *n)
{
    int *arr;
    int i;

    scanf("%d", n);

    arr = (int *)malloc((*n) * sizeof(int));
    if (arr == NULL)
    {
        exit(1);
    }

    for (i = 0; i < *n; i++)
    {
        scanf("%d", &arr[i]);
    }

    return arr;
}
