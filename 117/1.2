#include <stdio.h>
#include <stdlib.h>

void GetSet(int **data, int *num);

int main(void)
{
    int *data;
    int num;

    GetSet(&data, &num);

    free(data);
    return 0;
}

void GetSet(int **data, int *num)
{
    int i;

    scanf("%d", num);

    *data = (int *)malloc((*num) * sizeof(int));
    if (*data == NULL)
    {
        exit(1);
    }

    for (i = 0; i < *num; i++)
    {
        scanf("%d", &(*data)[i]);
    }
}

