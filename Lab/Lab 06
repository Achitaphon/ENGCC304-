#include <stdio.h>

int main() {
    int N;

    printf ( "Enter your number" );
    scanf ( "%d" , &N );

    if ( N % 2 == 1 ) {
       for ( int i = 1; i <= N; i += 2) {
           printf ("%d", i );
        }
} else {
    
        for ( int i = 0; i >= 0;i-= 2 ) {
           printf ("%d", i );
    }
}

    return 0; 

} 

