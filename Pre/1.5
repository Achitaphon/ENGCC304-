#include <stdio.h>

int main(void)
{
    int number;
    int evenSum;
    int oddSum;

    evenSum = 0;
    oddSum = 0;

    scanf("%d", &number);

    while (number != 0)
    {
        if (number % 2 == 0)
        {
            evenSum = evenSum + number;
        }
        else
        {
            oddSum = oddSum + number;
        }

        scanf("%d", &number);
    }

    printf("Even Sum: %d\n", evenSum);
    printf("Odd Sum: %d\n", oddSum);

    return 0;
}
