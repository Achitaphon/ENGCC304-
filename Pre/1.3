#include <stdio.h>

int main(void)
{
    int num1, num2;
    int operationCode;
    int result;

    scanf("%d", &num1);
    scanf("%d", &num2);
    scanf("%d", &operationCode);

    if (operationCode == 1)
    {
        result = num1 + num2;
        printf("%d", result);
    }
    else if (operationCode == 2)
    {
        result = num1 - num2;
        printf("%d", result);
    }
    else if (operationCode == 3)
    {
        result = num1 * num2;
        printf("%d", result);
    }
    else if (operationCode == 4)
    {
        result = num1 / num2;
        printf("%d", result);
    }
    else
    {
        printf("Invalid Operation");
    }

    return 0;
}
